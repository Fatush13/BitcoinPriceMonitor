<configuration>
   <appender name="STDOUTINFO" class="ch.qos.logback.core.ConsoleAppender">
      <encoder>
         <pattern>%msg%n</pattern>
      </encoder>
   </appender>
   <logger name="deng" level="DEBUG"/>
   <root level="INFO">
      <appender-ref ref="STDOUTINFO"/>
   </root>
</configuration>